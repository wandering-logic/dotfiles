#!/usr/bin/awk -f
BEGIN {
    OFS="\t"
    itercount = 0
    fwd_exists = 0
    iter_to_print = 5
    if (ARGC > 1) {
        iter_to_print = ARGV[1]
        ARGC = ARGC - 1
    }
}

/first_layer_fwd_kernel/ {
    fwd_exists = 1
    itercount = itercount + 1
}

/first_layer_wgrad_kernel/ {
    if (!fwd_exists) {
        if (itercount == 1) {
            print("WARNING: couldn't find `first_layer_fwd` using wgrad instead!")
        }
        itercount = itercount + 1
    }
}

{
    if (itercount == iter_to_print) {
        print
    }
}
